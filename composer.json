{
    "name": "jalso/google-analytics-bundle",
    "description": "Google certificate-based authentication in server-to-server interactions with google analytics",
    "keywords": [ "google", "analytics", "server" ],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Widop",
            "email": "contact@widop.com"
        },
        {
            "name": "Eric GELOEN",
            "email": "geloen.eric@gmail.com"
        },
        {
            "name": "jalso",
            "email": "jalso77@gmail.com"
        }
    ],
    "require": {
        "symfony/framework-bundle": "4.*",
        "jalso/http-adapter-bundle": "1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
	"repositories" : [
		 {
			"type" : "vcs",
			"url" : "https://github.com/jalso/WidopHttpAdapterBundle.git"
		}
		],
    "autoload": {
        "psr-0": { "Widop\\GoogleAnalyticsBundle": "" }
    },
    "config": {
        "bin-dir": "bin"
    },
    "target-dir": "Widop/GoogleAnalyticsBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        }
    }
}
